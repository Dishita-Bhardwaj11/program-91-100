#include <stdio.h>
#include <string.h>

int main() {
    char str[200], longest[50], current[50];
    int i, j = 0, maxLen = 0;

    fgets(str, sizeof(str), stdin);

    for (i = 0; str[i] != '\0'; i++) {
        if (str[i] != ' ' && str[i] != '\n') {
            current[j++] = str[i];
        } else {
            current[j] = '\0';
            if (j > maxLen) {
                maxLen = j;
                strcpy(longest, current);
            }
            j = 0;
        }
    }
    current[j] = '\0';
    if (j > maxLen) {
        strcpy(longest, current);
    }

    printf("%s", longest);
    return 0;
}
