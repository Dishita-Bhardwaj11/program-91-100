#include <stdio.h>
#include <string.h>

int 


Substring(char str[], char sub[]) {
    int i, j;
    for (i = 0; str[i] != '\0'; i++) {
        j = 0;
        while (str[i+j] == sub[j] && sub[j] != '\0') {
            j++;
        }
        if (sub[j] == '\0')
            return 1;
    }
    return 0;
}

int main() {
    char str1[100], str2[100], temp[200];

    scanf("%s", str1);
    scanf("%s", str2);

    if (strlen(str1) != strlen(str2)) {
        printf("Not rotation");
        return 0;
    }

    strcpy(temp, str1);
    strcat(temp, str1); 

    if (Substring(temp, str2))
        printf("Rotation");
    else
        printf("Not rotation");

-    return 0;
}
