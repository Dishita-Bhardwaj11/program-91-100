#include <stdio.h>
#include <string.h>

int main() {
    char str[200], word[100];
    int i, j = 0;

    fgets(str, sizeof(str), stdin);

    for (i = 0; str[i] != '\0'; i++) {
        if (str[i] != ' ' && str[i] != '\n') {
            word[j++] = str[i];
        } else {
            word[j] = '\0';
            for (int k = j - 1; k >= 0; k--)
                printf("%c", word[k]);

            printf(" ");
            j = 0;
        }
    }

    word[j] = '\0';
    for (int k = j - 1; k >= 0; k--)
        printf("%c", word[k]);

    return 0;
}
