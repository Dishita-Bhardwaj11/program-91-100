#include <stdio.h>
#include <ctype.h>

int main() {
    char str[200];
    int i;

    fgets(str, sizeof(str), stdin);
    if (str[0] != ' ')
        printf("%c.", toupper(str[0]));
    for (i = 1; str[i] != '\0'; i++) {
        if (str[i] == ' ' && str[i+1] != ' ' && str[i+1] != '\0' && str[i+1] != '\n') {
            printf("%c.", toupper(str[i+1]));
        }
    }

    return 0;
}
